# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Model Shelf Path
MODEL_SHELF_PATH=/path/to/your/model_shelf

# Deepgram API Key
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Hugging Face Token
HF_TOKEN=your_huggingface_token_here

# ASR Models Configuration (JSON string)
ASR_MODELS={"en-US-100m":{"uri":"your_asr_server:port","language_code":"en-US","model":"your_model_name"}}

# NLP Model URI
NLP_MODEL_URI=your_nlp_server:port

# MT Model URI
MT_MODEL_URI=your_mt_server:port

# Qdrant Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# TensorRT LLM Configuration (commented out as in original)
